{
  "name": "Ilaadeeg",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "test": "jest",
    "publish": "expo publish",
    "build:ios": "expo build:ios",
    "build:android": "cd android && ./gradlew app:assembleRelease",
    "build:bundle": "cd android && ./gradlew app:bundleRelease",
    "clean:android": "cd android && ./gradlew clean",
    "build:web": "expo build:web",
    "precommit": "lint-staged",
    "start:e2e": "expo start --no-dev --minify",
    "test:e2e": "./e2e/scripts/setup.sh && detox test --configuration ios.sim.release --retries 3 --loglevel trace --debug-synchronization 500",
    "postinstall": "patch-package",
    "prettier": "prettier --write \"{,.,**/,.jhipster/**/}*.{md,json,yml,js,ts,tsx}\"",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios  --simulator \"Ilaadeeg\"",
    "ios-tablet": "react-native run-ios  --simulator \"Lulpos\"",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "lint-staged": {
    "{,app/**/,test/**/}*.{js,json,yml,md,ts,tsx}": [
      "prettier --write",
      "eslint --fix --ext .js"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "app/shared/services/api.js",
      "app/shared/themes/",
      "/storybook/",
      "/*\\.story\\.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "preset": "jest-expo",
    "setupFiles": [
      "./test/setup"
    ],
    "testMatch": [
      "**/test/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/",
      "test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native-community|@react-native|react-native|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|@storybook))"
    ]
  },
